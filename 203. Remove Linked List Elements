struct ListNode* removeElements(struct ListNode* head, int val) {
    typedef struct ListNode ListNode;
    ListNode* vhead;
    vhead = (ListNode*)malloc(sizeof(ListNode));
    vhead->next = head;
    ListNode* cur = vhead;
    while (cur->next != NULL) {
        if (cur->next->val == val) {
            cur->next = cur->next->next;
        }
        else{
            cur = cur->next;
        }
    }
    head = vhead->next;
    free(vhead);
    return head;
}
